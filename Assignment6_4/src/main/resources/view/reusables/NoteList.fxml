<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="listView" maxWidth="100.0" prefWidth="100.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.NoteListController">

    <children>
      <VBox prefHeight="100.0" prefWidth="100.0" style="-fx-background-color: eeeeee; -fx-border-color: d6d6d6; -fx-border-radius: 5;">
         <children>
            <Label fx:id="listViewLabel" text="content" wrapText="true">
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
               <font>
                  <Font size="11.0" />
               </font>
            </Label>
         </children>
      </VBox>
        <Label fx:id="listViewTitle" alignment="CENTER" maxHeight="17.0" maxWidth="100.0" text="Title" wrapText="true" VBox.vgrow="ALWAYS">
            <VBox.margin>
                <Insets top="5.0" />
            </VBox.margin>
            <font>
                <Font name="System Bold" size="13.0" />
            </font>
        </Label>
    </children>

</VBox>
